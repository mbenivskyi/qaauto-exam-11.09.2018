<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="SearchTestSuite" verbose="1" >
    <parameter name="browserName" value="Firefox"/>
    <parameter name="envURL" value="https://google.com"/>
    <test name="GoogleSearchTestFirefox" >
        <classes>
            <class name="test.GoogleSearchTest" />
        </classes>
    </test>
    <parameter name="browserName" value="Chrome"/>
    <parameter name="envURL" value="https://google.com"/>
    <test name="GoogleSearchTestChrome" >
        <classes>
            <class name="test.GoogleSearchTest" />
        </classes>
    </test>
</suite>
